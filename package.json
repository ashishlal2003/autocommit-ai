{
  "name": "autocommit-ai",
  "displayName": "AutoCommit AI",
  "publisher": "LalAshish",
  "description": "This tool allows you to automate the process of committing git changes based on the user's local changes in the codebase.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocommit-ai.autocommitai",
        "title": "AutoCommit AI"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashishlal2003/autocommit-ai.git"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "dotenv": "^16.3.1"
  }
}
