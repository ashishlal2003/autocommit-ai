{
  "name": "autocommit-ai",
  "displayName": "AutoCommit AI",
  "description": "This tool allows you to automate the process of commiting git changes based on the user's local changes in the codebase.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:workbench.files.openFolder"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocommit-ai.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "dotenv": "^16.3.1"
  }
}
